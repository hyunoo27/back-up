function fnFilterFixed(a,b,c){$(a).each(function(){var a=$(this),e=$(this).parent(b),f=parseInt(a.css("top"));$(window).scroll(function(){setTimeout(function(){var b=$(window).scrollTop(),d=$(window).height(),k=e.offset().top,g=e.outerHeight(!0)-d+k;b>k-c&&a.outerHeight(!0)<e.outerHeight(!0)?a.outerHeight(!0)>d?b>g?a.css({position:"absolute",bottom:"auto",top:e.outerHeight(!0)-a.outerHeight(!0)}):$("body").hasClass("scrollDown")?b<a.offset().top+a.outerHeight(!0)-d-1?a.css({position:"absolute",bottom:"auto",
top:a.offset().top-k+"px"}):a.css({position:"fixed",top:"auto",bottom:"0px"}):b<=a.offset().top-c?a.css({position:"fixed",top:c+"px",bottom:"auto"}):a.css({position:"absolute",bottom:"auto",top:a.offset().top-k+"px"}):b>=g-c+d-a.outerHeight(!0)?a.css({position:"absolute",bottom:"auto",top:e.outerHeight(!0)-a.outerHeight(!0)+"px"}):a.css({position:"fixed",top:c+"px",bottom:"auto"}):a.css({position:"absolute",bottom:"auto",top:f+"px"})},10)})})}
function scrollCheck(a,b){function c(){var a=$(this).scrollTop();Math.abs(e-a)<=f||(a>e?$("body").removeClass("scrollUp").addClass("scrollDown"):a+$(window).height()<$(document).height()&&$("body").removeClass("scrollDown").addClass("scrollUp"),e=a)}var d,e=0,f=a;$(window).scroll(function(a){d=!0});setInterval(function(){d&&(c(),d=!1)},b)};