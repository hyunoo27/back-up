function getGoodsImagePath(c,n,k,a,b,d){var g="";if(null!=k&&""!=k)if(null!=a&&""!=a){for(var e=0;e<b.length;e++)if(Number(b[e])>=Number(a)){a=b[e];break}if("140"==d||"150"==d||"170"==d)g=c+"/prd/recimg/"+n.substring(n.length-3,n.length)+"/"+a+"/"+k.substring(k.lastIndexOf("/")+1);else if("10"==d||"30"==d)g=c+"/prd/img/"+n.substring(n.length-3,n.length)+"/"+a+"/"+k.substring(k.lastIndexOf("/")+1)}else g=c+k;return g};